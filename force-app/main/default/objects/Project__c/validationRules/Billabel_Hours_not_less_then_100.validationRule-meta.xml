<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billabel_Hours_not_less_then_100</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Billabel_Hours__c &lt;  100,
      ISBLANK( Billabel_Hours__c ), 
     $Setup.validationRuleConfiguration__c.Validation_Rules__c 
 )</errorConditionFormula>
    <errorDisplayField>Billabel_Hours__c</errorDisplayField>
    <errorMessage>Billabel Hours should not be less then 100</errorMessage>
</ValidationRule>
